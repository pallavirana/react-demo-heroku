{
  "name": "vishalshahi.com",
  "version": "1.0.0",
  "description": "Vishal Shahi Bio",
  "main": "webpack.config.js",
  "scripts": {
    "heroku-postbuild": "npm run build",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=local node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uCreate-Vishal/vishalshahi.com.git"
  },
  "keywords": [
    "Vishal",
    "Shahi"
  ],
  "author": "Vishal Shahi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uCreate-Vishal/vishalshahi.com/issues"
  },
  "homepage": "https://github.com/uCreate-Vishal/vishalshahi.com#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "group-css-media-queries-loader": "^1.0.4",
    "image-webpack-loader": "^3.3.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {}
}
